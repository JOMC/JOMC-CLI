    <implementation identifier="org.jomc.cli.Jomc"
                    class="org.jomc.cli.Jomc"
                    name="${pom.name}"
                    version="${pom.version}"
                    vendor="${pom.organization.name}"
                    final="true">

      <documentation defaultLanguage="en">
        <text language="en">JOMC command line interface.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0"
                mail="schulte2005@users.sourceforge.net" createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <dependencies>
        <dependency name="Commands"
                    identifier="org.jomc.cli.Command"
                    version="${pom.version}"
                    bound="false"/>

        <dependency name="Locale"
                    identifier="java.util.Locale"
                    version="1.1"
                    implementationName="default"/>

      </dependencies>
      <properties>
        <property name="width" type="int" value="80">
          <documentation defaultLanguage="en">
            <text language="en">The number of characters per line for the usage statement.</text>
          </documentation>
        </property>
        <property name="leftPad" type="int" value="2">
          <documentation defaultLanguage="en">
            <text language="en">The number of characters of padding to be prefixed to each line.</text>
          </documentation>
        </property>
        <property name="descPad" type="int" value="2">
          <documentation defaultLanguage="en">
            <text language="en">The number of characters of padding to be prefixed to each description line.</text>
          </documentation>
        </property>
        <property name="helpCommandName" value="help">
          <documentation defaultLanguage="en">
            <text language="en">The name of the command used to request help.</text>
          </documentation>
        </property>
      </properties>
      <messages>
        <message name="usage">
          <template defaultLanguage="en">
            <text language="en"><![CDATA[Type ''jomc <command> {0}'' for further information.]]></text>
            <text language="de"><![CDATA[Geben Sie ''jomc <Befehl> {0}'' für weitere Hilfe ein.]]></text>
          </template>
          <arguments>
            <argument index="0" name="helpCommandName"/>
          </arguments>
        </message>
        <message name="illegalArguments">
          <template defaultLanguage="en">
            <text language="en"><![CDATA[Illegal arguments. Try ''jomc {0} {1}''.]]></text>
            <text language="de"><![CDATA[Ungültige Argumente. Versuchen Sie ''jomc {0} {1}''.]]></text>
          </template>
          <arguments>
            <argument index="0" name="command"/>
            <argument index="1" name="helpCommandName"/>
          </arguments>
        </message>
      </messages>
    </implementation>
